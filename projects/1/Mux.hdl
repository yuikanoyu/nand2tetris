/** 
 * Multiplexor:
 * if (sel = 0) out = a, else out = b
 */
CHIP Mux {
    IN a, b, sel;
    OUT out;

    PARTS:
    Not(in=sel,out=w1);
    And(a=a,b=w1,out=w2);
    And(a=b,b=sel,out=w3);
    Or(a=w2,b=w3,out=out);
}